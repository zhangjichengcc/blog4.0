{
  "presets": ["next/babel"],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      },
      "antd"
    ],
    [
      "import",
      {
        "libraryName": "antd-mobile",
        "style": "css"
      },
      "antd-mobile"
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@": "./"
        }
      }
    ]
  ]
}
